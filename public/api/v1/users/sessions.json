{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "hackdashboard.heroku.com/api/v1/",
  "resourcePath": "sessions",
  "apis": [
    {
      "path": "users/sign_in",
      "operations": [
        {
          "summary": "Create a user session.",
          "parameters": [
            {
              "paramType": "form",
              "name": "user[email]",
              "type": "string",
              "description": "Email address",
              "required": true
            },
            {
              "paramType": "form",
              "name": "user[password]",
              "type": "string",
              "description": "Password",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            },
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            }
          ],
          "method": "post",
          "nickname": "Api::V1::Users::Sessions#create"
        }
      ]
    },
    {
      "path": "users/sign_out",
      "operations": [
        {
          "summary": "Destroy a user session.",
          "parameters": [
            {
              "paramType": "form",
              "name": "auth_token",
              "type": "string",
              "description": "Authentication token",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            }
          ],
          "method": "delete",
          "nickname": "Api::V1::Users::Sessions#destroy"
        }
      ]
    }
  ]
}